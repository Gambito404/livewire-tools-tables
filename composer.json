{
    "name": "gambito404/tools-tables",
    "description": "A dynamic table component for Laravel Livewire 3.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "gambito404",
            "email": "gambito404@example.com"
        }
    ],
    "require": {
